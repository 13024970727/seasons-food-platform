<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.seasonsfood.mall.servicemap.mapper.MapPenCoordinateMapper" >
  <resultMap id="BaseResultMap" type="com.seasonsfood.mall.servicemap.api.domain.MapPenCoordinate" >
    <id column="id" property="id" jdbcType="INTEGER" />
      <result column="lng" property="lng" jdbcType="DOUBLE" />
      <result column="lat" property="lat" jdbcType="DOUBLE" />
    <result column="areaId" property="area_id" jdbcType="INTEGER" />
      <result column="sortRankId" property="sort_rank_id" jdbcType="INTEGER" />
  </resultMap>


  <select id="selectByAreaId" parameterType="java.lang.Integer" resultMap="BaseResultMap">
  SELECT id,lng,lat,area_id,sort_rank_id FROM map_pen_coordinate where area_Id=#{areaId} order by sort_rank_id asc
     </select>
</mapper>